{
  "bail": 1,
  "verbose": false,
  "collectCoverageFrom": [
    "**/*.{js,jsx,ts,tsx}",
    "!**/node_modules/**",
    "!**/coverage/**",
    "!jest.config.js"
  ],
  "coverageDirectory": "coverage",
  "coverageThreshold": {
    "global": {
      "branches": 70,
      "functions": 70,
      "lines": 70
    }}
}